{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Tailscale ACL Policy Configuration",
  "description": "Schema for Tailscale tailnet policy file (v2). Supports grants, ACLs, SSH, postures, tags, autoApprovers, tests, sshTests, and more. Grants are the recommended approach.",
  "type": "object",
  "properties": {
    "acls": {
      "type": "array",
      "items": {
        "type": "object",
        "required": ["action", "src", "dst"],
        "properties": {
          "action": {
            "type": "string",
            "enum": ["accept"]
          },
          "src": {
            "type": "array",
            "items": {
              "oneOf": [
                { "type": "string", "pattern": "^autogroup:[a-z]+$" },
                { "type": "string", "pattern": "^group:[^@]+(@[a-zA-Z0-9.-]+)?$" },
                { "type": "string", "pattern": "^[^@]+@[^@]+\\.[^@]+$" },
                { "type": "string", "pattern": "^tag:[a-zA-Z0-9_-]+$" },
                { "type": "string", "pattern": "^(?:[0-9]{1,3}\\.){3}[0-9]{1,3}(?:/[0-9]{1,2})?$" },
                { "type": "string", "pattern": "^(?:[0-9a-fA-F:]+)(?:/[0-9]{1,3})?$" },
                { "type": "string", "pattern": "^[a-zA-Z0-9._-]+$" },
                { "type": "string", "const": "*" }
              ]
            },
            "minItems": 1
          },
          "dst": {
            "type": "array",
            "items": {
              "anyOf": [
                {
                  "type": "string",
                  "pattern": "^autogroup:(self|member|admin|network-admin|it-admin|billing-admin|auditor|owner|tagged|shared|internet)$"
                },
                { "type": "string", "pattern": "^group:[^@]+(@[a-zA-Z0-9.-]+)?$" },
                { "type": "string", "pattern": "^[^@]+@[^@]+\\.[^@]+$" },
                { "type": "string", "pattern": "^tag:[a-zA-Z0-9_-]+(:\\*|:([0-9]+(,[0-9]+)*))?$" },
                { "type": "string", "pattern": "^(?:[0-9]{1,3}\\.){3}[0-9]{1,3}(?:/[0-9]{1,2})?$" },
                { "type": "string", "pattern": "^(?:[0-9a-fA-F:]+)(?:/[0-9]{1,3})?$" },
                { "type": "string", "pattern": "^[a-zA-Z0-9._-]+$" },
                { "type": "string", "pattern": "^ipset:[a-zA-Z0-9_-]+$" },
                { "type": "string", "const": "*" },
                { "type": "string" }
              ]
            },
            "minItems": 1
          },
          "proto": {
            "type": ["number", "string"],
            "oneOf": [
              { "type": "number", "minimum": 1, "maximum": 255 },
              { "type": "string", "enum": ["tcp", "udp", "sctp", "icmp"] }
            ]
          }
        },
        "additionalProperties": false
      },
      "minItems": 0
    },
    "ssh": {
      "type": "array",
      "items": {
        "type": "object",
        "required": ["action", "src", "dst"],
        "properties": {
          "action": {
            "type": "string",
            "enum": ["accept", "check"]
          },
          "src": {
            "type": "array",
            "items": {
              "oneOf": [
                { "type": "string", "pattern": "^autogroup:[a-z]+$" },
                { "type": "string", "pattern": "^group:[^@]+(@[a-zA-Z0-9.-]+)?$" },
                { "type": "string", "pattern": "^[^@]+@[^@]+\\.[^@]+$" },
                { "type": "string", "pattern": "^tag:[a-zA-Z0-9_-]+$" }
              ]
            },
            "minItems": 1
          },
          "dst": {
            "type": "array",
            "items": {
              "oneOf": [
                { "type": "string", "const": "autogroup:self" },
                { "type": "string", "pattern": "^tag:[a-zA-Z0-9_-]+$" },
                { "type": "string", "pattern": "^[^@]+@[^@]+\\.[^@]+$" }
              ]
            },
            "minItems": 1
          },
          "users": {
            "type": "array",
            "items": {
              "oneOf": [
                { "type": "string", "enum": ["root", "autogroup:nonroot"] },
                { "type": "string", "pattern": "^[^@]+@[^@]+\\.[^@]+$" },
                { "type": "string", "pattern": "^localpart:\\*@[a-zA-Z0-9.-]+$" }
              ]
            },
            "minItems": 0
          },
          "checkPeriod": {
            "type": "string",
            "pattern": "^(?:[0-9]+[mhdw]|always)$"
          },
          "acceptEnv": {
            "type": "array",
            "items": {
              "type": "string",
              "pattern": "^[A-Za-z_][A-Za-z0-9_*?]*$"
            }
          }
        },
        "additionalProperties": false
      },
      "minItems": 0
    },
    "hosts": {
      "type": "object",
      "additionalProperties": {
        "type": "string",
        "pattern": "^(?:[0-9]{1,3}\\.){3}[0-9]{1,3}(?:/[0-9]{1,2})?$|^(?:[0-9a-fA-F:]+)(?:/[0-9]{1,3})?$"
      },
      "propertyNames": {
        "pattern": "^[a-zA-Z0-9._-]+$"
      },
      "minProperties": 0
    },
    "groups": {
      "type": "object",
      "additionalProperties": {
        "type": "array",
        "items": {
          "type": "string",
          "pattern": "^[^@]+@[^@]+\\.[^@]+$|^group:[^@]+(@[a-zA-Z0-9.-]+)?$"
        },
        "minItems": 1
      },
      "propertyNames": {
        "pattern": "^group:[^@]+(@[a-zA-Z0-9.-]+)?$"
      },
      "minProperties": 0
    },
    "tagOwners": {
      "type": "object",
      "additionalProperties": {
        "oneOf": [
          {
            "type": "array",
            "items": {
              "oneOf": [
                { "type": "string", "pattern": "^autogroup:(admin|network-admin)$" },
                { "type": "string", "pattern": "^group:[^@]+(@[a-zA-Z0-9.-]+)?$" },
                { "type": "string", "pattern": "^[^@]+@[^@]+\\.[^@]+$" },
                { "type": "string", "pattern": "^tag:[a-zA-Z0-9_-]+$" }
              ]
            },
            "minItems": 1
          },
          {
            "type": "array",
            "items": [],
            "minItems": 0,
            "maxItems": 0
          }
        ]
      },
      "propertyNames": {
        "pattern": "^tag:[a-zA-Z0-9_-]+$"
      },
      "minProperties": 0
    },
    "autoApprovers": {
      "type": "object",
      "properties": {
        "routes": {
          "type": "object",
          "additionalProperties": {
            "type": "array",
            "items": {
              "oneOf": [
                { "type": "string", "pattern": "^autogroup:[a-z]+$" },
                { "type": "string", "pattern": "^group:[^@]+(@[a-zA-Z0-9.-]+)?$" },
                { "type": "string", "pattern": "^[^@]+@[^@]+\\.[^@]+$" },
                { "type": "string", "pattern": "^tag:[a-zA-Z0-9_-]+$" }
              ]
            },
            "minItems": 1
          },
          "propertyNames": {
            "pattern": "^(?:[0-9]{1,3}\\.){3}[0-9]{1,3}(?:/[0-9]{1,2})?$"
          },
          "minProperties": 0
        },
        "exitNode": {
          "type": "array",
          "items": {
            "oneOf": [
              { "type": "string", "pattern": "^autogroup:[a-z]+$" },
              { "type": "string", "pattern": "^group:[^@]+(@[a-zA-Z0-9.-]+)?$" },
              { "type": "string", "pattern": "^[^@]+@[^@]+\\.[^@]+$" },
              { "type": "string", "pattern": "^tag:[a-zA-Z0-9_-]+$" }
            ]
          },
          "minItems": 1
        }
      },
      "additionalProperties": false,
      "minProperties": 0
    },
    "postures": {
      "type": "object",
      "additionalProperties": {
        "type": "array",
        "items": {
          "type": "string",
          "pattern": "^[a-zA-Z_][a-zA-Z0-9_:.-]*$"
        },
        "minItems": 1
      },
      "propertyNames": {
        "pattern": "^posture:[a-zA-Z0-9_-]+$"
      },
      "minProperties": 0
    },
    "nodeAttrs": {
      "type": "array",
      "items": {
        "type": "object",
        "required": ["target", "attr"],
        "properties": {
          "target": {
            "type": "array",
            "items": {
              "oneOf": [
                { "type": "string", "pattern": "^autogroup:[a-z]+$" },
                { "type": "string", "pattern": "^group:[^@]+(@[a-zA-Z0-9.-]+)?$" },
                { "type": "string", "pattern": "^[^@]+@[^@]+\\.[^@]+$" },
                { "type": "string", "pattern": "^tag:[a-zA-Z0-9_-]+$" },
                { "type": "string", "const": "*" }
              ]
            },
            "minItems": 1
          },
          "attr": {
            "type": "array",
            "items": {
              "type": "string",
              "pattern": "^[a-zA-Z][a-zA-Z0-9_.:-]*$"
            },
            "minItems": 1
          },
          "app": {
            "type": "object",
            "additionalProperties": {
              "type": "array",
              "items": {
                "type": "object"
              }
            }
          }
        },
        "additionalProperties": false
      },
      "minItems": 0
    },
    "tests": {
      "type": "array",
      "items": {
        "type": "object",
        "required": ["src"],
        "properties": {
          "src": {
            "type": "string",
            "pattern": "^[^@]+@[^@]+\\.[^@]+$|^autogroup:[a-z]+$|^group:[^@]+(@[a-zA-Z0-9.-]+)?$|^tag:[a-zA-Z0-9_-]+$"
          },
          "srcPostureAttrs": {
            "type": "object",
            "additionalProperties": {
              "type": "string"
            }
          },
          "proto": {
            "type": "string",
            "enum": ["tcp", "udp", "icmp"]
          },
          "accept": {
            "type": "array",
            "items": {
              "type": "string",
              "pattern": "^tag:[a-zA-Z0-9_-]+(:[0-9]+(,[0-9]+)*)?$|^(?:[0-9]{1,3}\\.){3}[0-9]{1,3}(?:/[0-9]{1,2})?(:[0-9]+(,[0-9]+)*)?$|^[a-zA-Z0-9._-]+(:[0-9]+(,[0-9]+)*)?$|^autogroup:(self|member|admin|network-admin|it-admin|billing-admin|auditor|owner|tagged|internet)(:[0-9]+(,[0-9]+)*)?$"
            },
            "minItems": 1
          },
          "deny": {
            "type": "array",
            "items": {
              "type": "string",
              "pattern": "^tag:[a-zA-Z0-9_-]+(:[0-9]+(,[0-9]+)*)?$|^(?:[0-9]{1,3}\\.){3}[0-9]{1,3}(?:/[0-9]{1,2})?(:[0-9]+(,[0-9]+)*)?$|^[a-zA-Z0-9._-]+(:[0-9]+(,[0-9]+)*)?$|^autogroup:(self|member|admin|network-admin|it-admin|billing-admin|auditor|owner|tagged|internet)(:[0-9]+(,[0-9]+)*)?$"
            },
            "minItems": 1
          }
        },
        "additionalProperties": false,
        "anyOf": [{ "required": ["accept"] }, { "required": ["deny"] }]
      },
      "minItems": 0
    },
    "sshTests": {
      "type": "array",
      "items": {
        "type": "object",
        "required": ["src", "dst"],
        "properties": {
          "src": {
            "type": "string",
            "pattern": "^[^@]+@[^@]+\\.[^@]+$|^autogroup:[a-z]+$|^group:[^@]+(@[a-zA-Z0-9.-]+)?$|^tag:[a-zA-Z0-9_-]+$"
          },
          "dst": {
            "type": "array",
            "items": {
              "oneOf": [
                { "type": "string", "const": "autogroup:self" },
                { "type": "string", "pattern": "^tag:[a-zA-Z0-9_-]+$" },
                { "type": "string", "pattern": "^[^@]+@[^@]+\\.[^@]+$" }
              ]
            },
            "minItems": 1
          },
          "accept": {
            "type": "array",
            "items": {
              "type": "string",
              "pattern": "^[^@]+@[^@]+\\.[^@]+$|root|autogroup:nonroot|^localpart:\\*@[a-zA-Z0-9.-]+$"
            },
            "minItems": 0
          },
          "check": {
            "type": "array",
            "items": {
              "type": "string",
              "pattern": "^[^@]+@[^@]+\\.[^@]+$|root|autogroup:nonroot|^localpart:\\*@[a-zA-Z0-9.-]+$"
            },
            "minItems": 0
          },
          "deny": {
            "type": "array",
            "items": {
              "type": "string",
              "pattern": "^[^@]+@[^@]+\\.[^@]+$|root|autogroup:nonroot|^localpart:\\*@[a-zA-Z0-9.-]+$"
            },
            "minItems": 0
          }
        },
        "additionalProperties": false,
        "anyOf": [{ "required": ["accept"] }, { "required": ["check"] }, { "required": ["deny"] }]
      },
      "minItems": 0
    },
    "ipsets": {
      "type": "object",
      "additionalProperties": {
        "type": "array",
        "items": {
          "oneOf": [
            { "type": "string", "pattern": "^(?:[0-9]{1,3}\\.){3}[0-9]{1,3}(?:/[0-9]{1,2})?$" },
            { "type": "string", "pattern": "^(?:[0-9a-fA-F:]+)(?:/[0-9]{1,3})?$" },
            { "type": "string", "pattern": "^[a-zA-Z0-9._-]+$" },
            { "type": "string", "pattern": "^autogroup:[a-z]+$" },
            { "type": "string", "pattern": "^ipset:[a-zA-Z0-9_-]+$" }
          ]
        },
        "minItems": 1
      },
      "propertyNames": {
        "pattern": "^ipset:[a-zA-Z0-9_-]+$"
      },
      "minProperties": 0
    },
    "derpMap": {
      "type": "object",
      "additionalProperties": true,
      "description": "Custom DERP server configuration. See Tailscale docs for structure."
    },
    "disableIPv4": {
      "type": "boolean"
    },
    "oneCGNATRoute": {
      "type": "string",
      "enum": ["", "mac-always", "mac-never"]
    },
    "randomizeClientPort": {
      "type": "boolean"
    }
  },
  "additionalProperties": false,
  "examples": [
    {
      "grants": [
        {
          "src": ["group:engineering"],
          "dst": ["tag:webserver"],
          "ip": ["*"]
        }
      ],
      "ssh": [
        {
          "action": "check",
          "src": ["autogroup:member"],
          "dst": ["autogroup:self"],
          "users": ["autogroup:nonroot"]
        }
      ],
      "hosts": {
        "db-server": "100.64.0.5"
      },
      "groups": {
        "group:engineering": ["alice@example.com", "bob@example.com"]
      },
      "tagOwners": {
        "tag:webserver": [],
        "tag:prod": ["group:sre"]
      },
      "autoApprovers": {
        "routes": {
          "192.168.1.0/24": ["group:network-admin"]
        },
        "exitNode": ["tag:exit-node-allowed"]
      },
      "postures": {
        "posture:secure-mac": ["node:os == 'macos'", "node:tsVersion >= '1.70'"]
      },
      "tests": [
        {
          "src": "alice@example.com",
          "accept": ["tag:webserver:80"],
          "deny": ["autogroup:internet:443"]
        }
      ],
      "sshTests": [
        {
          "src": "alice@example.com",
          "dst": ["autogroup:self"],
          "accept": ["alice"],
          "check": ["admin"],
          "deny": ["root"]
        }
      ],
      "ipsets": {
        "ipset:internal-networks": ["192.168.0.0/16", "10.0.0.0/8"]
      }
    }
  ]
}
